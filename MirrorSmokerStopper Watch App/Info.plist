<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleDevelopmentRegion</key>
	<string>en</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleIdentifier</key>
	<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleName</key>
	<string>$(PRODUCT_NAME)</string>
	<key>CFBundleDisplayName</key>
	<string>MirrorSmoker Watch</string>
	<key>CFBundlePackageType</key>
	<string>APPL</string>
	<key>CFBundleShortVersionString</key>
	<string>1.0.1</string>
	<key>CFBundleVersion</key>
	<string>4</string>
	<key>WKWatchKitApp</key>
	<true/>
	<key>WKCompanionAppBundleIdentifier</key>
	<string>com.mirror-labs.mirrorsmoker</string>
	<key>UIBackgroundModes</key>
	<array>
		<string>remote-notification</string>
	</array>
	<key>WKBackgroundModes</key>
	<array>
		<string>self-care</string>
		<string>workout-processing</string>
	</array>
	<key>UIAppFonts</key>
	<array>
		<string>JetBrainsMonoNL-Regular.ttf</string>
		<string>JetBrainsMonoNL-Medium.ttf</string>
		<string>JetBrainsMonoNL-SemiBold.ttf</string>
		<string>JetBrainsMonoNL-Bold.ttf</string>
	</array>
	<!-- Required HealthKit purpose strings (fix for App Store validation errors) -->
	<key>NSHealthShareUsageDescription</key>
	<string>The app reads your heart rate, activity, sleep and mindfulness data to anticipate smoking cravings and give you timely coaching directly on your Apple Watch.</string>
	<key>NSHealthUpdateUsageDescription</key>
	<string>The app can optionally log your smoking cessation milestones and mindful sessions to Health to keep all your wellness progress in one place.</string>
</dict>
</plist>
